<% if(products.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Id</th> <th>Nombre</th> <th>Orecio ($)</th> <th>Producto</th> </tr>
            <%products.forEach(function(product) {%>
            <tr>
                <td><%=product.id%></td>
                <td><%=product.title%></td>
                <td><%=product.price%></td>
                <td><%=product.thumbnail%></td>
            </tr>
            <%});%>
        </table>
    </div>
<% } else{ %>
    <h3 class="alert alert-danger">no se encontraron datos</h1>
<% } %>
